{
  "name": "aprs-weather-station",
  "version": "1.0.0",
  "description": "A simple weather station using NodeJS and OpenWeather API.",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node -r dotenv/config src/index.ts",
    "build": "tsc",
    "postinstall": "npm run build"
  },
  "keywords": [
    "aprs",
    "weather",
    "station",
    "node",
    "openweather"
  ],
  "author": "Diego Guevara",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "winston": "^3.17.0"
  }
}
